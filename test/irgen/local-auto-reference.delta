// RUN: %delta -print-ir %s | %FileCheck %s

class Foo { const i: int; }
func main() {
   // CHECK: %f = alloca %Foo
   // CHECK-NEXT: %rf = alloca %Foo*
   // CHECK-NEXT: %pf = alloca %Foo*
    const f: Foo = uninitialized;
    const rf: Foo& = f; // CHECK-NEXT: store %Foo* %f, %Foo** %rf
    const pf: Foo* = f; // CHECK-NEXT: store %Foo* %f, %Foo** %pf
}
