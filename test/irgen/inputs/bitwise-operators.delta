func and(a: int, b: int) -> int {
    return a & b;
}
func or(a: int, b: int) -> int {
    return a | b;
}
func xor(a: int, b: int) -> int {
    return a ^ b;
}
func shl(a: int, b: int) -> int {
    return a << b;
}
func ashr(a: int, b: int) -> int {
    return a >> b;
}
func lshr(a: uint, b: uint) -> uint {
    return a >> b;
}
func compl(a: int) -> int {
    return ~a;
}
