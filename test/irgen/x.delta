// RUN: %delta -print-ir %s | %FileCheck %s

interface I {
    func i();
}

struct S {
    let i: int;
    init() {}
    func i() {}
}

class A<E> {
    init() {}

    // CHECK: define %S @"A<S>.a"({}* %this)
    func a() -> E {
        // CHECK-NEXT: %1 = call %S @S.init()
        // CHECK-NEXT: ret %S %1
        return E();
    }
}

class M<T: I> {
    let t: T;

    init() {}

    // CHECK: define void @"M<S>.f"(%"M<S>"* %this)
    func f() {
        let a = A<T>();
        // CHECK: %2 = call %S @"A<S>.a"({}* %a)
        // CHECK-NEXT: call void @S.i(%S %2)
        a.a().i();
    }
}

func main() {
    let m = M<S>();
    m.f();
}
