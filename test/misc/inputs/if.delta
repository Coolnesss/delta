// compile_and_run_and_check_output $'a\nc\ne'

func main() {
    if (true) {
        puts("a");
    }

    if (false) {
        puts("b");
    } else {
        puts("c");
    }

    if (false) {
        puts("d");
    } else if (true) {
        puts("e");
    } else {
        puts("d");
    }
}

extern func puts(s: char*);
