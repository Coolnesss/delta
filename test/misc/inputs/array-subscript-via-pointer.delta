// compile_and_run_and_check_output "Foo"

extern func malloc(n: uint64) -> mutable void*;
extern func puts(s: char*);

func main() {
    let buffer = &*cast<mutable uint8[4]*>(malloc(4));
    buffer[0] = 70;
    buffer[1] = 0x6f;
    buffer[2] = 0o157;
    buffer[3] = 0b0;
    puts(cast<char*>(cast<void*>(buffer)));
}
