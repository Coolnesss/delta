struct StringIterator {
    var current: char*;
    let end: char*;

    init(string: StringRef) {
        current = string.data();
        end = string.data() + string.size();
    }

    func hasNext() -> bool {
        return current != end;
    }

    mutating func next() -> char {
        let next = current;
        current++;
        return *next;
    }
}
