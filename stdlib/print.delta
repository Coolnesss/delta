func print(text: StringRef) {
    printf("%.*s\n", text.size(), text.data());
}

func readLine() -> String {
    var line = String();

    while (true) {
        let character = getchar();
        // TODO: handle EOF
        if (character == '\n') { break; }
        line.push(char(character));
    }

    return line;
}
